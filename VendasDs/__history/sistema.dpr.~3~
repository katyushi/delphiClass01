program sistema;

uses
  Vcl.Forms,
  cliente in 'cliente.pas' {fCliente},
  produtos in 'produtos.pas' {Form2};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfCliente, fCliente);
  Application.CreateForm(TForm2, Form2);
  Application.Run;
end.
